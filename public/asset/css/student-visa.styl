#studentVisa
	margin-left:10%
	.heading-tabs
		border:0
		height:22px
		width: 80%
		margin-bottom:10px
		
		

		li
			padding: 0
			margin: 0
			width:15%
			text-align: center
			border-right:1px solid #929497
			height:21px


			&:nth-child(1)
				width:19%

			&:nth-child(4)
				.icons
					background-position: 30% 0

			&:nth-child(5)
				.icons
					background-position: 63% 0
	

			&:last-child
				border-right:0
					
				.icons
					background-position: 100% 0



			a
				color:#929497
				font-size:14pt
				line-height: 21px



				&:hover
					text-decoration: none
	h3
		margin-top:0
		padding-top:20px
		padding-bottom:10px
		font-size:16pt
		color:#262261
		border-bottom:1px solid #929497
		
		
	.sidebar
		min-height:750px
		border-top:1px solid #929497
		h3
			font-weight:800


		ul
			
			
			border-bottom:0

			li
				padding:5px
				padding-left:10px
				margin:0
				&.active
					
					background-color:#262261
					font-size:14pt
					margin:0

					a
						background-color:#262261
						color:#fff
						&:hover,&:focus
							border:0
							outline:0
							background-color:#262261
							border-radius:0
							padding:5px
							margin:0


				a
					font-size:14pt
					color:#929497
					padding:5px
					margin:0
					border-radius:0
					outline:0
					border:0
						

					&:hover,&:focus
						border:0
						outline:0
						background-color:transparent
						border-radius:0
						padding:5px
						margin:0

		.categories-content
			border-top:2px solid #929497
			padding-top:5px

			h3
				background-color:#929497
				color:#fff
				padding-left:12px
				padding-top:10px
			ul
				padding-left:12px
				padding-right:12px
				li
					padding-left:0
					padding-right:0
					border-bottom:1px solid #929497


					&.active
						background-color:transparent
					
						a
							background-color:transparent
							color:#262261
				

				
					
	.content
		padding-left:2%
		border-top:1px solid #929497
